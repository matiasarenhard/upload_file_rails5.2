<%= form_with(model: example, local: true, multipart: true) do |form| %>
<div class="row justify-content-center align-items-center">
  <br>
    <div class="col-sm-4 field">
        <%= form.label :file %>
        <%= form.file_field :file %>
    </div>
    <div class="col-sm-1">
      <div class="actions">
        <%= form.submit "Upload", class: "btn btn-info" %>
      </div>
    </div>
  </div>
<% end %>
